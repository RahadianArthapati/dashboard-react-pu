{
  "name": "dva-loading",
  "version": "0.2.3",
  "description": "Auto loading plugin for dva.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvajs/dva-loading"
  },
  "homepage": "https://github.com/dvajs/dva-loading",
  "keywords": [
    "dva",
    "dva-plugin",
    "loading"
  ],
  "author": "chencheng <sorrycc@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --no-timeouts",
    "debug": "mocha --require babel-register --require test/setup --no-timeouts",
    "build": "rimraf lib && babel src --out-dir lib",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "./test/setup"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "^6.14.0",
    "coveralls": "^2.11.11",
    "cross-env": "^2.0.1",
    "dva": "^1.1.0",
    "expect": "^1.20.2",
    "jsdom": "^9.6.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "react": "^15.3.2",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "dva": "^1.1.0 || ^1.3.0-0"
  },
  "files": [
    "lib",
    "src"
  ]
}
